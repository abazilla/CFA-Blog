<h1>Admin Dashboard</h1>

<table>
  <thead>
    <tr>
      <td>ID</td>
      <td>Email</td>
      <td>User Type</td>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.email %></td>
      <td><%= user.user_type %></td>
    </tr>
    <% end %>
  </tbody>
</table>
